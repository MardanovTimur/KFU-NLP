URLS	first.py	/^URLS = [$/;"	v
tetxs_untokenized	first.py	/^tetxs_untokenized = map(lambda x: Text(x).collocations(),texts)$/;"	v
texts	first.py	/^texts = map(lambda x: word_tokenize(nltk.clean_html(urlopen(x).read().decode('utf-8'))),URLS)$/;"	v
